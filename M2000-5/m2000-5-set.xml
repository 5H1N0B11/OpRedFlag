<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList include="m2000-5-base.xml">

  <sim>

    <description>Mirage 2000 (YASim)</description>

    <author>BARANGER Emmanuel (3D/FDM), Buckaroo (FDM), 5H1N0B1 (Weapons, system), F-Sig and hardball (3D, ssystems)</author>
    
    <aircraft-version>2015-09-16</aircraft-version>
    <status>production</status>

    <flight-model>yasim</flight-model>

    <aero>m2000-5-yasim</aero>

    <rating>
      <FDM     type="int"> 4 </FDM>
      <systems type="int"> 4 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model   type="int"> 4 </model>
    </rating>

  </sim>

  <logging>
      <log>
       <filename>C:/Users/Nikolai/Documents/m2000_flightlog.csv</filename>
       <enabled>false</enabled>
       <interval-ms>500</interval-ms>
       <delimiter>,</delimiter>
       <entry>
        <enabled>true</enabled>
        <title>Latitude</title>
        <property>/position/latitude-deg</property>
       </entry>
       <entry>
        <enabled>true</enabled>
        <title>Longitude</title>
        <property>/position/longitude-deg</property>
       </entry>
       <entry>
        <enabled>true</enabled>
        <title>alt ft</title>
        <property>/position/altitude-ft</property>
       </entry>
       <entry>
        <enabled>true</enabled>
        <title>date</title>
        <property>logging/date-log</property>
       </entry>
       <entry>
        <enabled>true</enabled>
        <title>time</title>
        <property>logging/time-log</property>
       </entry>
      </log>
      <time-log type="string">0:0:0</time-log>
      <date-log type="string">0/0/0</date-log>
     </logging>

</PropertyList>
